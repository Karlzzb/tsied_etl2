{
  "query": {
    "filtered": {
      "query": {
        "query_string": {
          "query": "*",
          "analyze_wildcard": true
        }
      },
      "filter": {
        "bool": {
          "must": [
            {
              "range": {
                "@timestamp": {
                  "gte": 1456761600000,
                  "lte": 1459439999999,
                  "format": "epoch_millis"
                }
              }
            }
          ],
          "must_not": []
        }
      }
    }
  },
  "size": 0,
  "aggs": {
    "date_aggs": {
      "date_histogram": {
        "field": "@timestamp",
        "interval": "1d",
        "time_zone": "Asia/Hong_Kong",
        "min_doc_count": 1,
        "extended_bounds": {
          "min": 1456761600000,
          "max": 1459439999999
        }
      },
      "aggs": {
        "ad_domain": {
          "terms": {
            "field": "referer.raw",
            "size": 0,
            "order": {
              "_count": "desc"
            }
          },
          "aggs": {
            "uv_stats": {
              "cardinality": {
                "field": "user_id.raw"
              }
            },
            "ip_stats": {
              "cardinality": {
                "field": "clientip.raw"
              }
            },
            "session_stats": {
              "cardinality": {
                "field": "session_id.raw"
              }
            }
          }
        }
      }
    }
  }
}