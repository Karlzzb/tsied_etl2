<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>  
        <setting name="cacheEnabled" value="true"/>  
        <setting name="callSettersOnNulls" value="true"/>
        <setting name="defaultStatementTimeout" value="15000" />  
    </settings> 
    <typeAliases>
    	<typeAlias type="com.etl.dao.entity.TestMysql" alias="_test"/>
    	<typeAlias type="com.etl.dao.entity.GameMysql" alias="_game"/>
    	<typeAlias type="com.etl.dao.entity.AdPayStats" alias="_pay"/>
    </typeAliases>
    <mappers>
    	<mapper resource="mybatisConfig/test.xml"/>
    	<mapper resource="mybatisConfig/game.xml"/>
    	<mapper resource="mybatisConfig/pay.xml"/>
    </mappers>

</configuration> 